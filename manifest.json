{
  "name": "MapsLink",
  "short_name": "MapsLink",
  "description": "Conectando profissionais e empresas através de um mapa interativo e inteligente.",
  "lang": "pt-BR",
  "dir": "ltr",
  "start_url": "/index.html?source=pwa",
  "scope": "/",
  "display": "standalone",
  "orientation": "portrait",
  "background_color": "#ffffff",
  "theme_color": "#102569",
  "categories": ["business", "maps", "productivity"],
  "prefer_related_applications": false,
  "icons": [
    {
      "src": "assets/images/logo-icon-192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "assets/images/logo-icon-512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "screenshots": [
    {
      "src": "assets/images/3d_avatar_21.png",
      "sizes": "220x220",
      "type": "image/png",
      "label": "Tela inicial do MapsLink"
    },
    {
      "src": "assets/images/image 4.png",
      "sizes": "137x137",
      "type": "image/png",
      "label": "Mapa interativo de empresas"
    }
  ],
  "shortcuts": [
    {
      "name": "Abrir Mapa",
      "short_name": "Mapa",
      "description": "Ver empresas e vagas no mapa interativo",
      "url": "/pages/paginamapav4.html",
      "icons": [
        {
          "src": "assets/images/logo-icon-192.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Ver Vagas",
      "short_name": "Vagas",
      "description": "Navegar pelas oportunidades dispon��veis",
      "url": "/pages/vagas.html",
      "icons": [
        {
          "src": "assets/images/logo-icon-192.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "$schema": "https://json.schemastore.org/web-manifest-combined.json",
  "id": "https://mapslink.app/",
  "display_override": ["standalone", "fullscreen"],
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+mapslink",
      "url": "/pages/paginamapav4.html?q=%s"
    }
  ],
  "share_target": {
    "action": "/pages/contact.html",
    "method": "POST",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "title": "subject",
      "text": "message",
      "url": "map"
    }
  }
}
